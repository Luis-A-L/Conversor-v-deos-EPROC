{
  "name": "compressor-eproc",
  "productName": "TJPR Compressor EPROC",
  "version": "2.1.0",
  "description": "Ferramenta segura de compressão de vídeo para o sistema E-PROC (TJPR).",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [
    "eproc",
    "compressor",
    "ffmpeg",
    "electron"
  ],
  "author": "TJPR",
  "license": "UNLICENSED",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "electron": "^28.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "icon.ico",
        "asar": false
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "CompressorEPROC"
          }
        },
        {
          "name": "@electron-forge/maker-zip"
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2"
  }
}
